{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [
      1,
      "double",
      { "avoidEscape": true, "allowTemplateLiterals": true }
    ],
    "no-console": 0,
    "radix": 1,
    "max-nested-callbacks": [2, 3],
    "no-new-wrappers": 2,
    "no-sequences": 2,
    "no-var": 2,
    "max-params": [1, 3],
    "max-depth": [1, 3],
    //"max-len": [2, 80, 4],
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "curly": 0,
    "no-multi-spaces": 1,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-debugger": 0
  },
  "globals": {
    "performance": true
  }
}
